    <Implementation>
      <ST><![CDATA[systemtaskinfo();

ulogger1(
	bEnable:= TRUE, 
	nBufferSizeSet:= 10240, 
	bEnableEventLogger:= , 
	bEnableFileLogger:= , 
	sLogFilePath:= 'C:\');
