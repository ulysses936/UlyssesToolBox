tonWriteLog(IN:= NOT tonWriteLog.Q, PT:= T#300MS, Q=> , ET=> );
IF tonWriteLog.Q AND bTestUlogger THEN
	ulogger1.WriteLog(String1);
	ulogger1.WriteLog(String1);
	ulogger1.WriteLog(String1);
END_IF
systemappinfo();
fileLogger(
	Overwrite:= , 
	SizeOfMemory:= 512, 
	TimeFillBuffer=> , 
	Full=> , 
	Empty=> , 
	ReadyToRead=> , 
	pDataToRead=> , 
	SizeOfDataToRead=> , 
	hr=> , 
